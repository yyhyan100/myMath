    "# plot of the weight function\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "x=np.linspace(0,0.999,30)\n",
    "w=1/np.sqrt(1-x**2)\n",
    "plt.xlim(0,1)\n",
    "plt.ylim(1,2)\n",
    "plt.scatter(x,w)"
