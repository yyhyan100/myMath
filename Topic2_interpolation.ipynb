   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "x_cor=np.array([0, 1, 2, 3, 4, 5], dtype=np.double)   # x coordinates of the points\n",
    "y_cor=np.array([2, 3, 6, 10,16, 27], dtype=np.double)   # y coordinates of the points\n",
    "x=3.5       # we need get the interpolated value at 3.5\n",
    "n=x_cor.shape[0]\n",
    "dd=np.zeros((n,n), dtype=np.double)    # matrix for the divided differences\n",
    "dd[:,0]=y_cor   # first column is the function values\n",
    "\n",
    "for j in range(1,n):\n",
    "    for i in range(j,n):\n",
    "        dd[i,j]=(dd[i-1,j-1]-dd[i,j-1])/(x_cor[i]-x_cor[i-j])    # the divided difference at ith row , jth col\n",
    "\n",
    "b=dd.diagonal()  # coefficients of the polynamial\n",
    "\n",
    "y=b[0]\n",
    "for i in range(1,len(b)):   # loop for the terms in the polynomial\n",
    "    tmp=1.0\n",
    "    for j in range(i):   # loop for the product of the factors in each term of the polynomial\n",
    "        tmp*=(x-x_cor[j])\n",
    "    tmp*=b[i]\n",
    "    y+=tmp \n",
    "\n",
    "plt.scatter(x_cor,y_cor, c=\"blue\")\n",
    "plt.plot(x_cor,y_cor)\n",
    "plt.scatter([x],[y], c=\"red\")\n"
   ]
